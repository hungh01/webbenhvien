@model IEnumerable<WebBookingCare.Models.EF.DichVu>

<section id="services" class="services section-bg">
    <div class="container">
        <div class="section-title">
            <h2>Dịch Vụ</h2>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-md-6 d-flex align-items-stretch mt-4">
                    <div class="icon-box">
                        <div class="icon"><img src="@item.HinhAnh"/></div>
                        <h4><a href="">@item.TenDV</a></h4>
                        <p>@item.MoTa</p>
                    </div>
                </div>
            }
        </div>
    </div>
</section>